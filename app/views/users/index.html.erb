<div class="row justify-content-center">
      <h1 class="col-4">Resultats</h1>
</div
<% @user.each do |user| %>
      <%= link_to user_path(user.id) do %> 
            <div class="card w-80 mb-3">
                  <div class="card-body">
                        <h5 class="card-title"><%=user.nom%></h5>
                        <p class="card-text"><%=user.presentation%></p>
                  </div>
            </div
      <% end %>  
<% end %>
