<p id="notice"><%= notice %></p>

<h1>Projets</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th>Contenu</th>
      <th>Debut</th>
      <th>Fin</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projets.each do |projet| %>
      <tr>
        <td><%= projet.nom %></td>
        <td><%= projet.contenu %></td>
        <td><%= projet.debut %></td>
        <td><%= projet.fin %></td>
        <td><%= projet.user_id %></td>
        <td><%= link_to 'Show', projet %></td>
        <td><%= link_to 'Edit', edit_projet_path(projet) %></td>
        <td><%= link_to 'Destroy', projet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projet', new_projet_path %>
